    music: {fileID: 0}
    sound: {fileID: 0}
  - text: "\u300C\u2026\u90A3\u9EBC\u679C\u7136\u53EA\u80FD\u5F9E\u9019\u4E9B\u5B57\u4E0B\u624B\u4E86\u55CE\uFF1F\u300D"
